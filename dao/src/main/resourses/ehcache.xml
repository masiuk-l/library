<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
    <defaultCache maxEntriesLocalHeap="20" eternal="false"
                  timeToIdleSeconds="120" timeToLiveSeconds="200" memoryStoreEvictionPolicy="LRU"/>

    <cache name="org.hibernate.cache.StandardQueryCache" maxEntriesLocalHeap="5" eternal="false"
           timeToLiveSeconds="120">
        <persistence
                strategy="localTempSwap"/>
    </cache>
</ehcache>